#ifndef COMMON_H
#define COMMON_H

#include <map>
#include <vector>

using namespace std;

typedef struct {
  string name;
  string type;
} Variable;

// varName, type
map <string, string> varTable;
// funcName, agruments
map <string, vector<Variable> > functionTable;

void f();
void emit(string singleInstruction);

#endif //COMMON_H
